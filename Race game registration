let raceNumber = Math.floor(Math.random() * 1000)
let registerEarly=true
let age=45
if (age>18 && registerEarly=true){
  console.log('Runner is Early adult')
  raceNumber+=1000

}

  if (age>18 && registerEarly=true){
  console.log( `Race starts at 9:30 am and raceNumber is ${raceNumber}`)
  }
  else if (age>18 && registerEarly!=true){
    console.log(`Race with age ${age} starts at 11 am and race number is ${raceNumber}`)
  }

  else if (age<18){
    console.log(`Race with age ${age} starts at 12:30 pm and race number is ${raceNumber}`)
  }
  else{
    console.log("Check registration desk")
  }

 


